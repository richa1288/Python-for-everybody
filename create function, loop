def addtwo(a, b):
    added = a + b
    return added

x = addtwo(3, 5)
print(x)


# Loop
while True:
    line = input('> ')
    if line == 'done':
        break
    print(line)
print('Done!')


# String
while True:
    line = input('> ')
    if line[0] == '#':
        continue
    if line == 'done':
        break
    print(line)
print('Done!')
